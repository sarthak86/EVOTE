            <div class="main-panel">
                <div class="content-wrapper" style="background-color: #e0f6f8;">
                    <div class="page-header">
                        <h3 class="page-title">
                            <span class="page-title-icon bg-gradient-info text-white mr-2">
                                <i class="mdi mdi-home"></i>
                            </span>
                            Dashboard
                        </h3>
                    </div>
                    <div class="page-header">
                        <div class="container">
                            <div class="col-12 grid-margin stretch-card">
                                <div class="card">
                                    <div class="card-body">
                                        <h4 class="card-title">Company details</h4>
                                        <p class="card-description">
                                            Basic informations
                                        </p>
                                                
                                        <form class="forms-sample" action="/companyDetails/edit/<%= AllCompanies._id %>"
                                            class="btn btn-gradient-info mr-2" method="POST">
                                            <div class="form-group">
                                                <label for="exampleInputName1">Company Name</label>
                                                <input type="text" class="form-control" id="exampleInputName1"
                                                    placeholder="Name" value="<%= AllCompanies.companyName %>"
                                                    name="companyName" disabled>

                                            </div>
                                            <div class="form-group">
                                                <label for="exampleInputEmail3">Company Email address</label>
                                                <input type="email" class="form-control" id="companyEmail"
                                                    placeholder="Email" value="<%= AllCompanies.companyEmail %>"
                                                    name="companyEmail" required>
                                            </div>
                                            <div class="form-group">
                                                <label for="exampleInputEmail3">Password</label>
                                                <input type="password" class="form-control" id="companyPassword"
                                                    placeholder="Password" value="<%= AllCompanies.companyPassword %>"
                                                    name="companyPassword" required>
                                            </div>
                                            <div class="form-group">
                                                <label for="exampleInputPassword4">Phone no</label>
                                                <input type="text" class="form-control" id="companyPhoneNumber"
                                                    placeholder="Phone no" value="<%= AllCompanies.companyPhoneNumber %>"
                                                    name="companyPhoneNumber" required  pattern="[0-9]{10}" title="Enter 10 digit phone number" >
                                            </div>
                                            
                                            
                                            
                                                <div class="form-group">
                                                    <label for="exampleInputCity1">City</label>
                                                    <input type="text" class="form-control" id="companyCity"
                                                        placeholder="Location" value="<%= AllCompanies.companyCity %>"
                                                        name="companyCity" required>
                                                </div>
                                                <!-- <button type="submit" class="btn btn-gradient-info mr-2"></button> -->

                                                <button class="btn btn-gradient-info mr-2">Submit</button>

                                                <!-- <button type="submit" onclick="myfunc1()" class="btn btn-gradient-info mr-2">Delete</button> -->
                                                
                                                <button class="btn btn-light">Cancel</button>
                                        </form>
                                    
                                </div>
                            </div>
                        </div>
                    </div>
                </div>

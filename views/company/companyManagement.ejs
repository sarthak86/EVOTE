            <div class="col-lg-12 grid-margin stretch-card">
              <div class="card">
                <div class="card-body">
                  <h4 class="card-title">Company Managment</h4>
                  <table class="table table-striped">
                    <thead>
                      <tr>
                        <th>
                          Company Logo
                        </th>
                        <th>
                          Company Id
                        </th>
                        <th>
                          Company Name
                        </th>
                        <th>
                          Company Email
                        </th>
                      
                        <th>
                          Phone no
                        </th>
                        
                      </tr>
                    </thead> 
                    <tbody>
                            <% AllCompanies.forEach(function(comp){ %>
                                
                                <tr style="background-color: #e0f6f8;">
                                        <td class="py-1">
                                          <img src= "<%= '/images/Company/' +  comp.companyLogo %>" alt="">
                                        </td>
                                        <td>
                                                <%= comp._id %> 
                                        </td>
                                        <td>
                                                <%= comp.companyName %> 
                                        </td>
                                        <td>
                                          <%= comp.companyEmail %> 
                                  </td>
                              
                                        <td>
                                                <%= comp.companyPhoneNumber %> 
                                        </td>
                                  
                                      </tr> 
                                      <tr>
                                        <td></td> 
                                      </tr>                                                            
                                <%  }) %>


                                
                    </tbody>
                  </table>
                </div>
